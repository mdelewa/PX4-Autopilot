#!/bin/sh
#
# PX4 FMUv4 specific board MAVLink startup script.
#------------------------------------------------------------------------------

# Pixracer: start MAVLink on Wifi (ESP8266 port). Except for the TealOne airframe.
if ! param compare SYS_AUTOSTART 4250
then
	mavlink start -r 20000 -b 921600 -d /dev/ttyS0
fi
